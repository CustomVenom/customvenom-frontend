{
  "name": "customvenom-frontend",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --noEmit",
    "pre-commit": "npm run lint && npm run type-check",
    "prepare": "husky install",
    "postinstall": "prisma generate && node -e \"try{require('@tailwindcss/oxide/install')()}catch(e){process.exit(0)}\"",
    "backfill-analytics": "npx tsx scripts/backfill-analytics.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lhci": "lhci autorun --config=./lighthouse/lighthouserc.json"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.0",
    "@playwright/test": "^1.56.1",
    "@prisma/client": "^6.17.1",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@sentry/nextjs": "^8.0.0",
    "@stripe/stripe-js": "^4.0.0",
    "@tailwindcss/oxide-linux-x64-gnu": "latest",
    "@testing-library/react": "^16.3.0",
    "@vercel/analytics": "^1.5.0",
    "next": "15.5.4",
    "next-auth": "^4.24.11",
    "react": "^19.2.0",
    "react-dom": "19.2.0",
    "stripe": "^16.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@lhci/cli": "^0.12.0",
    "@opennextjs/cloudflare": "^1.9.1",
    "@storybook/addon-essentials": "^8.3.0",
    "@storybook/addon-interactions": "^8.3.0",
    "@storybook/addon-links": "^8.3.0",
    "@storybook/nextjs": "^8.3.0",
    "@storybook/react": "^8.3.0",
    "@storybook/test": "^8.3.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.14",
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "husky": "^9.1.7",
    "lightningcss": "^1.30.2",
    "lint-staged": "^16.2.4",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prisma": "^6.17.1",
    "storybook": "^8.3.0",
    "tailwindcss": "^4.1.14",
    "typescript": "5.9.3",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "optionalDependencies": {
    "@parcel/watcher-linux-x64-glibc": "^2.5.0",
    "@parcel/watcher-linux-x64-musl": "^2.5.0"
  },
  "overrides": {
    "lightningcss": "^1.30.2"
  },
  "react": "19.2.0",
  "react-dom": "19.2.0"
}
