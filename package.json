{
  "name": "customvenom-frontend",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "20"
  },
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "start:test": "cross-env PORT=3000 next start",
    "ci:deterministic": "npx -y npm@10.7.0 ci",
    "lint": "cross-env ESLINT_USE_FLAT_CONFIG=true eslint . --ext .ts,.tsx --max-warnings=0",
    "lint:fix": "cross-env ESLINT_USE_FLAT_CONFIG=true eslint . --ext .ts,.tsx --fix",
    "lint:staged": "eslint --fix",
    "typecheck": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "qa": "npm run typecheck && npm run lint",
    "pre-commit": "npm run lint && npm run type-check",
    "preflight:grep": "powershell -Command \"if (git grep -n 'http://.*yahoo\\.com' -- src 2>$null) { Write-Error 'Found http:// Yahoo references'; exit 1 }; if (git grep -n 'process\\.env\\.[A-Z_]' -- src 2>$null) { Write-Error 'Found dot-access env vars - use bracket access'; exit 1 }\"",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "test:gate": "npm run build && start /B cmd /C \"npm run start:test\" && timeout /t 3 >NUL && npx playwright test tests/trust-snapshot.spec.ts tests/health.spec.ts && taskkill /IM node.exe /F >NUL 2>&1",
    "preflight": "npm run preflight:grep && eslint . --max-warnings=2 && npm run type-check && npm run test:unit",
    "preflight:contracts": "npm run build && start /B cmd /C \"npm run start:test\" && timeout /t 5 >NUL && npx playwright test tests/contract-tests.spec.ts && taskkill /IM node.exe /F >NUL 2>&1",
    "cors:test": "curl -sSD - \"$NEXT_PUBLIC_API_BASE/health\" -H \"Origin: https://www.customvenom.com\" -o /dev/null | sed -n '1,30p'",
    "drift-watch": "powershell -ExecutionPolicy Bypass -File scripts/drift-watch.ps1",
    "prepare": "husky install || echo 'husky skipped'",
    "postinstall": "prisma generate && node -e \"try{require('@tailwindcss/oxide/install')()}catch(e){process.exit(0)}\"",
    "backfill-analytics": "npx tsx scripts/backfill-analytics.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lhci": "lhci autorun --config=./lighthouse/lighthouserc.json"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.0",
    "@playwright/test": "^1.56.1",
    "@prisma/client": "^6.17.1",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@sentry/nextjs": "^8.0.0",
    "@stripe/stripe-js": "^4.0.0",
    "@testing-library/react": "^16.3.0",
    "@vercel/analytics": "^1.5.0",
    "next": "15.5.6",
    "next-auth": "^4.24.11",
    "react": "^19.2.0",
    "react-dom": "19.2.0",
    "stripe": "^16.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@eslint/js": "^9.17.0",
    "@lhci/cli": "^0.12.0",
    "@opennextjs/cloudflare": "^1.9.1",
    "@storybook/addon-essentials": "^8.3.0",
    "@storybook/addon-interactions": "^8.3.0",
    "@storybook/addon-links": "^8.3.0",
    "@storybook/nextjs": "^9.1.15",
    "@storybook/react": "^8.3.0",
    "@storybook/test": "^8.3.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.14",
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@typescript-eslint/utils": "^8.0.0",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "husky": "^9.1.7",
    "lightningcss": "^1.30.2",
    "lint-staged": "^16.2.4",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prisma": "^6.17.1",
    "storybook": "^8.3.0",
    "tailwindcss": "^4.1.14",
    "typescript": "5.9.3",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ]
  },
  "optionalDependencies": {
    "@parcel/watcher-linux-x64-glibc": "^2.5.0",
    "@parcel/watcher-linux-x64-musl": "^2.5.0"
  },
  "overrides": {
    "lightningcss": "^1.30.2"
  },
  "react": "19.2.0",
  "react-dom": "19.2.0"
}
