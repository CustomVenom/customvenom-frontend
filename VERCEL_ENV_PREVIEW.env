# Stripe Product/Price IDs (create in Stripe Dashboard)
# NEXT_PUBLIC_STRIPE_PRO_PRICE_ID=price_xxx
# NEXT_PUBLIC_STRIPE_TEAM_PRICE_ID=price_yyy

# Sentry - Staging (low sample rate)
# Get from: https://sentry.io/settings/[org]/projects/[project]/keys/
# SENTRY_DSN=https://[key]@[org].ingest.us.sentry.io/[project]
# NEXT_PUBLIC_SENTRY_DSN=https://[key]@[org].ingest.us.sentry.io/[project]
# SENTRY_SAMPLE_RATE=0.1
# SENTRY_TRACES_SAMPLE_RATE=0.05
SENTRY_DSN=
NEXT_PUBLIC_SENTRY_DSN=

# Feature Flags
NEXT_PUBLIC_DEMO_MODE=1
PAYWALL_DISABLED=0

# Logs - Disabled for preview
