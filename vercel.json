{
  "env": {
    "NEXT_PUBLIC_API_BASE": "https://api.customvenom.com",
    "NEXT_PUBLIC_ENABLE_YAHOO": "true"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_API_BASE": "https://api.customvenom.com",
      "NEXT_PUBLIC_ENABLE_YAHOO": "true"
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=31536000; includeSubDomains; preload"
        }
      ]
    }
  ]
}